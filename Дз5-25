#include <iostream>
#include <cmath>

using namespace std;

bool IsSquare(int K) {
    int root = sqrt(K);
    return (root * root == K);
}

int main() {
    int count = 0;

    for (int i = 0; i < 10; i++) {
        int number;

        // Read a positive integer from the user
        cin >> number;

        // Check if the number is a square
        if (IsSquare(number)) {
            cout<<"1"<<" ";
        }else{
            cout<<"0"<<" ";
        }
    }

    return 0;
}
